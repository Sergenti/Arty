#:kivy 1.0
#:import kivy kivy

<CollectionGridImage>:
    size_hint: 1,1

    RelativeLayout:
        CheckBox:
            id: select_image
            size_hint: (None, None)
            size: (dp(35), dp(35))
            pos_hint: {'left': 1, 'top': 1}
            
            background_checkbox_normal: 'resources/empty.png'#'resources/empty.png'
            background_checkbox_down: 'resources/check-box.png'
            on_active: root.checkbox_click(self, self.active) 